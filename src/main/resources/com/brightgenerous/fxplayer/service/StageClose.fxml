<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.control.Label?>
<VBox xmlns:fx="http://javafx.com/fxml"
    stylesheets="com/brightgenerous/fxplayer/service/StageClose.css"
    fx:id="rootPane"
    prefWidth="240" prefHeight="160"
    style="-fx-padding:5 5 5 5;-fx-background-color:rgb(248, 248, 248);">
  <Region VBox.vgrow="ALWAYS" />
  <HBox alignment="CENTER">
    <Label text="%confirm.text" />
  </HBox>
  <Region VBox.vgrow="ALWAYS" />
  <HBox alignment="CENTER" spacing="10">
    <Button fx:id="controlOk"
        text="%control.ok"  ellipsisString="control.ok.ellipsis" mnemonicParsing="true" />
    <Button fx:id="controlCancel"
        text="%control.cancel"  ellipsisString="control.cancel.ellipsis" mnemonicParsing="true"
        onAction="#controlCancel" />
  </HBox>
  <Region VBox.vgrow="ALWAYS" />
</VBox>
