<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml"
    stylesheets="com/brightgenerous/fxplayer/service/CloseOwnerStage.css"
    fx:id="rootPane"
    prefWidth="240" prefHeight="160">
  <Region VBox.vgrow="ALWAYS" />
  <HBox alignment="CENTER">
    <Label text="%confirm.text" />
  </HBox>
  <Region VBox.vgrow="ALWAYS" />
  <HBox alignment="CENTER" spacing="10">
    <Button text="%control.ok" ellipsisString="control.ok.ellipsis" mnemonicParsing="true"
        onAction="#controlOk" />
    <Button fx:id="controlCancel"
        text="%control.cancel" ellipsisString="control.cancel.ellipsis" mnemonicParsing="true"
        onAction="#controlCancel" />
  </HBox>
  <Region VBox.vgrow="ALWAYS" />
</VBox>
